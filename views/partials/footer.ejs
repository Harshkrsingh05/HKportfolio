<script>
  function updateTime() {
      fetch('/time') // Send a request to the server to get the current time
          .then(response => response.text())
          .then(time => {
              document.getElementById('time-display').innerText = time +' GMT+5:30'; // Update the displayed time
          });
  }

  // Call updateTime function immediately to update time when the page loads
  updateTime();

  // Update the time every second
  setInterval(updateTime, 30000);
</script>
